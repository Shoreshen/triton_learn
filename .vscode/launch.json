    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "Clear Cache",
            "env": {
                // "MLIR_ENABLE_DUMP": "1",
                // "LLVM_IR_ENABLE_DUMP": "1",
                // "TRITON_ENABLE_LLVM_DEBUG": "1",
                "TRITON_CACHE_DIR": "${workspaceFolder}/.cache",
                "TMPDIR": "${workspaceFolder}/.tmp"
            },
        },
        {
            "name": "Python & cpp: matmul_case_1.py",
            "type": "pythoncpp",
            "request": "launch",
            "pythonLaunchName": "Python: matmul_case_1.py",
            "cppAttachName": "(gdb) Attach",
        },
        {
            "name": "Python: matmul_case_1.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tests/matmul_case_1.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "Clear Cache",
            "env": {
                // "MLIR_ENABLE_DUMP": "1",
                // "LLVM_IR_ENABLE_DUMP": "1",
                // "TRITON_ENABLE_LLVM_DEBUG": "1",
                "TRITON_CACHE_DIR": "${workspaceFolder}/.cache",
                "TMPDIR": "${workspaceFolder}/.tmp"
            },
            // "args": "1>${workspaceFolder}/matmul_case_1.ll 2>&1"
        },
        {
            "name": "Python: 01-vector-add",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/triton-project/python/tutorials/01-vector-add.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "Clear Cache",
            "env": {
                "MLIR_ENABLE_DUMP": "1",
                "LLVM_IR_ENABLE_DUMP": "1",
                // "TRITON_ENABLE_LLVM_DEBUG": "1",
                "TRITON_CACHE_DIR": "${workspaceFolder}/.cache",
                "TMPDIR": "${workspaceFolder}/.tmp"
            },
            "args": "1>${workspaceFolder}/01-vector-add.ll 2>&1"
        },
        {
            "name": "Python: 02-fused-softmax",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/triton-project/python/tutorials/02-fused-softmax.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "Clear Cache",
            "env": {
                // "MLIR_ENABLE_DUMP": "1",
                // "LLVM_IR_ENABLE_DUMP": "1",
                // "TRITON_ENABLE_LLVM_DEBUG": "1",
                "TRITON_CACHE_DIR": "${workspaceFolder}/.cache",
                "TMPDIR": "${workspaceFolder}/.tmp"
            },
            "args": "1>${workspaceFolder}/02-fused-softmax.ll 2>&1"
        },
        {
            "name": "Python: 03-matrix-multiplication",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/triton-project/python/tutorials/03-matrix-multiplication.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "Clear Cache",
            "env": {
                // "MLIR_ENABLE_DUMP": "1",
                // "LLVM_IR_ENABLE_DUMP": "1",
                // "TRITON_ENABLE_LLVM_DEBUG": "1",
                "TRITON_CACHE_DIR": "${workspaceFolder}/.cache",
                "TMPDIR": "${workspaceFolder}/.tmp"
            },
            // "args": "1>${workspaceFolder}/03-matrix-multiplication.ll 2>&1"
        },
        {
            "name": "Python: triton_compile_test.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tests/triton_compile_test.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "preLaunchTask": "Clear Cache",
            "env": {
                // "MLIR_ENABLE_DUMP": "1",
                // "LLVM_IR_ENABLE_DUMP": "1",
                // "TRITON_ENABLE_LLVM_DEBUG": "1",
                "TRITON_CACHE_DIR": "${workspaceFolder}/.cache",
                "TMPDIR": "${workspaceFolder}/.tmp"
            },
            "args": "1>${workspaceFolder}/triton_compile_test.ll 2>&1"
        },
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/.venv/bin/python",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}